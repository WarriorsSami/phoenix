{
  "database": {
    "host": "localhost",
    "port": "5432",
    "user": "postgres",
    "password": "<PASSWORD>",
    "name": "phoenix"
  },
  "server": {
    "host": "localhost",
    "port": "8080"
  },
  "api": {
    "jwt": "<JWT_SECRET>",
    "authCookieLifeTime": 7
  },
  "eval": {
    "isolatePath": "/var/local/lib/isolate",
    "maxSandboxes": 10,

    "compilePath": "/tmp/pnBinaries",
    "outputPath": "/tmp/pnOutputs"
  },
  "languages": {
    "c": {
      "extension": ".c",
      "isCompiled": true,
      "compile": [
        "gcc",
        "-std=c11",
        "-O2",
        "-s",
        "-static",
        "/box/main.c",
        "-o",
        "/box/output"
      ],
      "execute": [
        "./main"
      ],
      "sourceFile": "main.c",
      "executable": "main"
    }
  }
}